package org.get.employeeSort;

public class LinkedList {
	private Node head;
	
	LinkedList(){
		this.head = null;
	}
	/**
	 * 
	 * @return getter for head node
	 */
	public Node getHead(){
		return this.head;
	}
	/**
	 * 
	 * @param node node to be inserted at end 
	 * 		  of linked list
	 */
	public void insert(Node node){
		if(head == null){
			Node newNode = new Node(node.getEmployee());
			head = newNode;
		}
		else{
			Node iterate = this.getHead();
			while(iterate.getNext() != null){
				iterate = iterate.getNext();
			}
			Node newNode = new Node(node.getEmployee());
			iterate.setNext(newNode);
		}
	}
}
