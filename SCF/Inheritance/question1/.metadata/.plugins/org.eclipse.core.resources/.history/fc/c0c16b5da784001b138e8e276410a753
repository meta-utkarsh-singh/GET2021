package com.get.zoomanagement;

public abstract class Animal {
	private String name;
	private int age;
	private int weight;
	private int id;
	
	Animal(String name, int age, int weight, int id){
		this.name = name;
		this.age = age;
		this.weight = weight;
		this.id =id;
	}
	/**
	 * 
	 * @return getter for animal name
	 */
	public String getName(){
		return this.name;
	}
	/**
	 * 
	 * @return getter for age
	 */
	public int getAge(){
		return this.age;
	}
	/**
	 * 
	 * @return getter for weight
	 */
	public int getWeight(){
		return this.weight;
	}
	/**
	 * 
	 * @return getter for id
	 */
	public int getId(){
		return this.id;
	}
}
